<%= form_for [@concert] do |f| %>
  <%= f.label :artist %>
  <%= f.text_field :artist %>
    <%= f.label :city %>
  <%= f.text_field :city %>
  <%= f.label :venue %>
  <%= f.text_field :venue %>
  <%= f.label :date %>
  <%=f.datetime_select :date, :ampm => true, :start_minute => [0,15,30,45] %>
  <%= f.label :price %>
  <%= f.number_field :price %>
  <%= f.label :description %>
  <%= f.text_field :description %>
  <%= f.submit "Add" %>
<% end %>

